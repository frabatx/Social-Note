<h1>Sharing note</h1>
<div class="container border border-dark shadow-lg p-4 mb-4 bg-white">
<div class="well well-lg">
<%= form_with url: create_sharing_note_path do |form| %>

<div class="form-group row">
<%= collection_check_boxes(:users, :user_ids, @user, :id, :email , :class => "form-control") %>
</div>

<div class="actions">
    <%= form.submit  %>
</div>
<% end %>
</div>
</div>
